@section scripts{
    <script type="text/javascript">

    $(document).ready(function () {
        //loadLoginDlg();
        //getAllPrintedDJs();
        //getReviews();

        $("#loader1").hide();
        $("#loader").hide();
        $('#tblPrinted').DataTable({
            "paging": true,
